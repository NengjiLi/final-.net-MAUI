<?xml version="1.0" encoding="utf-8" ?>
    
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp5.Pages.Member"
             Title="Member Registration">

    <StackLayout>
        <Entry x:Name="FirstNameEntry" Placeholder="First Name" />
        <Entry x:Name="LastNameEntry" Placeholder="Last Name" />
        <Entry x:Name="PhoneNumberEntry" Placeholder="Phone Number" />
        <Entry x:Name="EmailEntry" Placeholder="Email" />
        <Picker x:Name=" MembershipTypePicker" Title="  Membership Type">
            <Picker.ItemsSource>
                <x:Array Type="{x:Type x:String}">
                    <x:String>Basic</x:String>
                    <x:String>Premium</x:String>
                  
                </x:Array>
            </Picker.ItemsSource>
        </Picker>
        <Button Text="Generate Member ID"  />
        <Label x:Name="MemberIdLabel" Text="" />
    </StackLayout>
    
</ContentPage>